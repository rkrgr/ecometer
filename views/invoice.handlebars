<table class="invoice">
    <thead>
        <tr>
            <th colspan="5">Rechnungen</th>
        </tr>
    </thead>
    <tbody>
        {{#each invoices}}
        <tr>
            <td>
                <span>{{this.invoiceName}}</span>
            </td>
            <td>
                <span>{{this.invoiceValue}}</span>
            </td>
            <td>
                <span>{{this.emissionFactor}}</span>
            </td>
            <td>
                <span>{{formatDateTime this.dateStart}}</span>
            </td>
            <td>
                <span>{{formatDateTime this.dateEnd}}</span>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>