<table class="newsTicker">
    <thead>
        <tr>
            <th colspan="2">News-Ticker</th>
        </tr>
    </thead>
    <tbody>
        {{#each latestMeasures}}
        <tr>
            <td>
                <span>{{formatDateTime this.timestamp}}</span>
            </td>
            <td rowspan="2">
                <span>{{this.name}}</span>
            </td>
        </tr>
        <tr>
            <td>
                <span>{{this.companyId}}</span>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<table id="bestMeasures">
    <thead>
        <tr>
            <th colspan="2">Beste Ma√ünahmen</th>
        </tr>
    </thead>
    <tbody>
        {{#each bestMeasures}}
        <tr>
            <td>
                <span>{{formatDateTime this.timestamp}}</span>
            </td>
            <td>
                <span>{{this.name}}</span>
            </td>
        </tr>
        <tr>
            <td>
                <span>{{this.companyId}}</span>
            </td>
            <td>
                <span>{{this.co2Einsparung}} t CO2 eingespart</span>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
